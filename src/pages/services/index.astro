<!doctype html>
<html lang="it">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Redirectâ€¦</title>
    <meta http-equiv="refresh" content="0; url=/it/servizi/" />
  </head>
  <body>
    <script>
      (function(){
        try{
          var langs=['it','en','sl']
          var saved=localStorage.getItem('lang')||'it'
          if(langs.indexOf(saved)===-1) saved='it'
          var target='/' + saved + (saved === 'it' ? '/servizi/' : saved === 'sl' ? '/storitve/' : '/services/')
          location.replace(target)
        }catch(e){ location.replace('/it/servizi/') }
      })()
    </script>
    <noscript>
      <a href="/it/servizi/">Vai a /it/servizi/</a>
    </noscript>
  </body>
  </html>


