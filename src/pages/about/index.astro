  <body>
    <script>
      (function(){
        try{
          var langs=['it','en','sl']
          var saved=localStorage.getItem('lang')||'it'
          if(langs.indexOf(saved)===-1) saved='it'
          var map={ it: '/chi-sono/', en: '/about/', sl: '/about/' }
          var target='/' + saved + (map[saved]||'/about/')
          location.replace(target)
        }catch(e){ location.replace('/it/chi-sono/') }
      })()
    </script>
    <noscript>
      <a href="/it/chi-sono/">Vai a /it/chi-sono/</a>
    </noscript>
