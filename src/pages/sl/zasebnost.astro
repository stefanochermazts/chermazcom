---
import UnifiedLayout from '../../layouts/UnifiedLayout.astro'
import { getEntry, render } from 'astro:content'
const entry = await getEntry('pages', 'sl-privacy')
const Content = entry ? (await render(entry)).Content : null
---

<UnifiedLayout useLocalizedMeta={true} pageKey="privacy">
  {Content && (
    <section class="py-16 sm:py-20">
      <div class="mx-auto max-w-[1366px] px-4 sm:px-6 lg:px-8">
        <Content />
      </div>
    </section>
  )}
</UnifiedLayout>
